{{#unless errors}}
  <ul class="collection with-header">
    <li class="collection-header"><h4>Users</h4></li>
    {{#each data}}
      {{>admin/userManagement/usersListRow 
        _id=_id
        role=role
        email=email
        createdAt=createdAt
        csrfToken=../csrfToken
      }}
    {{/each}}
  </ul>
{{/unless}}